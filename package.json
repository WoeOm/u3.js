{
  "name": "u3.js",
  "version": "0.0.1",
  "description": "Node wrapper for Ultrain API",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=development mocha test/index.test.js --timeout 10000",
    "docs": "node test/gendoc.js && rm -rf docs && mkdir docs && node_modules/.bin/jsdoc -c jsdoc.json",
    "es5": "rm -fr es5 && babel --copy-files src --out-dir es5",
    "build": "rm -rf dist && mkdir dist && browserify -o dist/u3.js -s U3 es5/index.js && uglifyjs dist/u3.js -o dist/u3.min.js --source-map --compress --mangle"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ultrain-os/nodejs-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ultrain-os/nodejs-sdk/issues"
  },
  "keywords": [
    "web-service"
  ],
  "author": "ben.yasin80@gmail.com",
  "contributors": [],
  "engines": [
    "node >= 8"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bigi": "^1.4.2",
    "binaryen": "^37.0.0",
    "bip39": "^2.5.0",
    "browserify-aes": "^1.0.6",
    "bs58": "^4.0.1",
    "bytebuffer": "^5.0.1",
    "camel-case": "^3.0.0",
    "create-hash": "^1.1.3",
    "create-hmac": "^1.1.6",
    "ecurve": "^1.0.5",
    "fcbuffer": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "localStorage": "^1.0.3",
    "lodash.isempty": "^4.4.0",
    "lodash.isstring": "^4.0.1",
    "minimatch": "^3.0.4",
    "node-localstorage": "^1.3.1",
    "randombytes": "^2.0.5"
  },
  "devDependencies": {
    "@types/node": "^8.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "blanket": "*",
    "bluebird": "^3.5.0",
    "browserify": "^14.4.0",
    "camel-case": "^3.0.0",
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "docdash": "^1.0.0",
    "documentation": "^5.3.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "3.5.0",
    "nyc": "^11.7.1",
    "pedding": "*",
    "should": "1.2.2",
    "travis-cov": "*",
    "uglify-js": "3.4.1"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
